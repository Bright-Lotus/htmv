{
	"name": "htmv",
	"main": "dist/index.js",
	"type": "module",
	"version": "0.0.64",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./http": {
			"types": "./dist/http/index.d.ts",
			"default": "./dist/http/index.js"
		}
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.3",
		"@types/bun": "latest"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@elysiajs/node": "^1.4.2",
		"@elysiajs/static": "^1.4.6",
		"elysia": "^1.4.18"
	},
	"bin": {
		"htmv": "dist/cli/cli.js"
	},
	"scripts": {
		"build": "tsc",
		"lint": "biome check",
		"prepublishOnly": "bun run build",
		"test": "bun test",
		"test:gen:tokenizer": "bun run src/compiler/tests/tokenizer/gen.ts",
		"test:gen:renderer": "bun run src/compiler/tests/renderer/gen.ts",
		"test:gen:parser": "bun run src/compiler/tests/parser/gen.ts"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Fabrisdev/htmv"
	},
	"keywords": [
		"framework",
		"web-framework",
		"web",
		"server",
		"backend",
		"frontend",
		"cli"
	],
	"license": "MIT",
	"description": "A simple yet fast web framework"
}
